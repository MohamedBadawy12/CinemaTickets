@model IEnumerable<ApplicationUser>

@{
	ViewData["Title"] = "Lis of all users";
}
<div class="row">
	<div class="col-md-6 offset-3">
		<P>
			<h4>
				List Of All Users
			</h4>
		</P>
		<table class="table">
			<thead>
				<tr class="text-center">
					<th>Full Name</th>
					<th>User Name</th>
					<th>Email</th>
				</tr>
			</thead>
			<tbody>
				@foreach(var item in Model)
				{
					<tr>
						<td class="align-middle">
							@item.FullName
						</td>
						<td class="align-middle">
							@item.UserName
						</td>
						<td class="align-middle">
							@item.Email
						</td>
					</tr>
				}
			</tbody>
		</table>
	</div>
</div>